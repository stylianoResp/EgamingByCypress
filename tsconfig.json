{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "types": ["cypress", "jquery"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "cypress/**/*.ts"
  ]
}
